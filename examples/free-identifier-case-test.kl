#lang "prelude.kl"

(import (shift "prelude.kl" 1))
(import (shift "free-identifier-case.kl" 1))

(define-macros
  ([test
    (lambda (stx)
     (syntax-case stx
       [(list (_ x))
        (free-identifier-case x
          [if (pure '(the Integer 1))]
          [else (pure '(the Integer 2))]
          [define (pure '(the Integer 3))]
          [#%app (pure '(the Integer 4))]
          [(else y) (pure y)])]))]))

(define hello (the Integer 32))
(example (test if))
(example (test else))
(example (test define))
(example (test #%app))
(example (test hello))
